{
	// Place your snippets for typescriptreact here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Private method": {
		"prefix": "prvm",
		"body": [
			"private _$1() {",
			"\t$2",
			"}"
		],
		"description": "Private method"
	},
	"Private async method": {
		"prefix": "pravm",
		"body": [
			"private async _$1() {",
			"\t$2",
			"}"
		],
		"description": "Private async method"
	},
	"describe jest method": {
		"prefix": "jdsc",
		"body": [
			"// #region $1",
			"",
			"describe(\"$1\", () => {",
			"\t$2",
			"});",
			"",
			"// #endregion $1"
		],
		"description": "Stubs out a describe call for a Jest test method"
	},
	"test jest method": {
		"prefix": "jit",
		"body": [
			"test(\"$1\", (done) => {",
			"\t// Arrange & Act",
			"\tconst wrapper = $2();",
			"",
			"\t// Assert",
			"\texpect($3).toBe($4);",
			"\tdone();",
			"});"
		],
		"description": "Stubs out an 'test' call for a Jest test method"
	},
	"test jest method - no arrange/act": {
		"prefix": "jita",
		"body": [
			"test(\"$1\", (done) => {",
			"\t// Assert",
			"\texpect($2).toBe($3);",
			"\tdone();",
			"});"
		],
		"description": "Stubs out an 'test' call for a Jest test method, without arrange/act section"
	},
	"wrapper.debug();": {
		"prefix": "jdb",
		"body": [
			"console.log(wrapper.debug());",
		],
		"description": "Stubs out a console log to print the debug tree of current wrapper"
	},
	"beforeEach(() => {});": {
		"prefix": "jbe",
		"body": [
			"beforeEach(() => {",
			"\t",
			"});",
		],
		"description": "Stubs out a beforeEach() function to run for a describe() block"
	},
